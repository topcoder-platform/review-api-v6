		{
			"name": "4) Get Phases → capture IDs",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test('Get phases 200', function(){ pm.expect(pm.response.code).to.eql(200); });",
							"const data = pm.response.json();",
							"function findId(name){",
							"  if (!data || !Array.isArray(data)) return null;",
							"  const p = data.find(x => (x.name||'').toLowerCase() === name.toLowerCase());",
							"  return p && (p.id || p.phaseId);",
							"}",
							"pm.collectionVariables.set('registrationPhaseId', findId('Registration'));",
							"pm.collectionVariables.set('submissionPhaseId', findId('Submission'));",
							"pm.collectionVariables.set('reviewPhaseId', findId('Review'));",
							"pm.collectionVariables.set('appealsPhaseId', findId('Appeals'));",
							"pm.collectionVariables.set('appealsResponsePhaseId', findId('Appeals Response'));",
							"pm.test('phase IDs captured', function(){",
							"  pm.expect(!!pm.collectionVariables.get('registrationPhaseId')).to.be.true;",
							"  pm.expect(!!pm.collectionVariables.get('submissionPhaseId')).to.be.true;",
							"});"
						],
						"type": "text/javascript",
